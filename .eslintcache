urrentModule}\n        currentModuleIndex={currentModuleIndex}\n        handleJumpToSection={handleJumpToSection}\n        handleCurrentSection={handleCurrentSection}\n        handleCurrentModule={handleCurrentModule}\n      />\n    </U.Main>\n  );\n};\n\nexport default Activity;\n","/Users/karen/Code/theCOOP/project-colab/src/components/TuxComponents/FormComponents.js",[],"/Users/karen/Code/theCOOP/project-colab/src/components/SignupForm/EmailForm.jsx",[],"/Users/karen/Code/theCOOP/project-colab/src/components/SignupForm/GeneralForm.jsx",[],"/Users/karen/Code/theCOOP/project-colab/src/pages/HeuristicsActivity/SideBarNav.jsx",[],"/Users/karen/Code/theCOOP/project-colab/src/components/GroupUserTable/GroupUserTable.jsx",[],"/Users/karen/Code/theCOOP/project-colab/src/components/CreateGroup/CreateGroup.jsx",[],"/Users/karen/Code/theCOOP/project-colab/src/components/InviteModal/InviteModal.jsx",["224","225"],"/Users/karen/Code/theCOOP/project-colab/src/components/ActivityHeader/ActivityHeader.jsx",[],"/Users/karen/Code/theCOOP/project-colab/src/components/ActivityBody/ActivityBody.jsx",[],"/Users/karen/Code/theCOOP/project-colab/src/components/EmailCheckbox/EmailCheckbox.jsx",["226"],"/Users/karen/Code/theCOOP/project-colab/src/components/Question/Question.jsx",["227"],"import React, { useState, useEffect } from 'react';\nimport Feedback from '../Feedback/Feedback';\nimport * as U from '../TuxComponents/UniversalComponents';\nimport * as A from '../TuxComponents/ActivitiesComponents';\n\nconst Question = ({ details }) => {\n  const [response, setResponse] = useState('');\n  // const [bkgrdColor, setbkgrdColor] = useState(['blue', 'green', 'red'])\n\n  const { problemStatement, image, answers } = details.contents;\n\n  const handleResponse = (answer, index) => {\n    setResponse({ selection: answer, selectionIndex: index });\n  };\n\n  return (\n    <U.Sub6ColGrid>\n      <A.ProblemStatement>{problemStatement}</A.ProblemStatement>\n      <U.FlexBox spaceAround>\n        